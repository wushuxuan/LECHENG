/**app.wxss**/
@import 'src/wxss/index.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 


@font-face {
  font-family: 'iconfont';  /* project id 484502 */
  src: url('//at.alicdn.com/t/font_484502_39ze82luzlkawcdi.eot');
  src: url('//at.alicdn.com/t/font_484502_39ze82luzlkawcdi.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_484502_39ze82luzlkawcdi.woff') format('woff'),
  url('//at.alicdn.com/t/font_484502_39ze82luzlkawcdi.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_484502_39ze82luzlkawcdi.svg#iconfont') format('svg');
}


.iconfont{
    font-family:"iconfont" !important;
    font-size:16px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
.icon-search:before { content: "\e60b"; }/*搜索*/
.icon-left:before { content: "\e601"; }/*分割*/